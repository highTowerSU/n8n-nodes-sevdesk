{
  "name": "@ceneos.net/n8n-nodes-sevdesk",
  "version": "0.3.6",
  "description": "This is an n8n community node. It let's you interact with the sevDesk API. SevDesk is an accounting tool based in germany. You'll find more information on their [website](https://sevdesk.de/).",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://ceneos.net",
  "author": {
    "name": "Christian Debertsh√§user",
    "email": "see-imprint@ceneos.net"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highTowerSU/n8n-nodes-sevdesk.git"
  },
  "main": "index.ts",
  "scripts": {
    "build": "tsc && node -r ts-node/register ./node_modules/gulp/bin/gulp.js build:icons",
    "dev": "tsc --watch",
    "format": "node -r ts-node/register ./scripts/run-prettier.ts",
    "lint": "node -r ts-node/register ./node_modules/eslint/bin/eslint.js -c .eslintrc.ts nodes credentials package.json",
    "lintfix": "node -r ts-node/register ./node_modules/eslint/bin/eslint.js -c .eslintrc.ts nodes credentials package.json --fix",
    "lint:prepublish": "node -r ts-node/register ./node_modules/eslint/bin/eslint.js -c .eslintrc.prepublish.ts nodes credentials package.json",
    "prepublishOnly": "npm run build && npm run lint:prepublish"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SevDeskApi.credentials.ts"
    ],
    "nodes": [
      "dist/nodes/SevDesk/SevDesk.node.ts"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/prettier": "^2.7.3",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "fast-glob": "^3.3.2",
    "gulp": "^5.0.1",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
